{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "style.css",
        "use": "@vercel/static"
      },
      {
        "src": "app.js",
        "use": "@vercel/static"
      },
      {
        "src": "img/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/img/(.*)",
        "dest": "/img/$1"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/style.css",
        "dest": "/style.css"
      },
      {
        "src": "/app.js",
        "dest": "/app.js"
      }
    ]
  }
  